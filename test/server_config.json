{
  "port": 8090,
  "node_name": "node-01",
  "peers": [],
  "cluster_token": "secret-token-123",
  "security": {
    "blacklisted_commands": ["rm", "mkfs", "dd", "reboot", "shutdown", ":(){:|:&};:"],
